#include<iostream>
using namespace std;
int  main(){
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        string arr[n];
        for(int i=0;i<n;i++){
            cin>>arr[i];
        }
        int A=0,B=0;
        for(int i=0;i<n;i++){
            if(arr[i]=="START38"){
                A++;
            }
            else{
                B++;
            }
        }
        cout<<A<<" "<<B<<endl;
    }
}
