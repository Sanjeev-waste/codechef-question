#include<iostream>
using namespace std;
int main(){
    int t;
    cin>>t;
    while(t--){
        long long  n;
        cin>>n;
        long long  arr[n];
        for(long long i=0;i<n;i++){
            cin>>arr[i];
        }
        bool k=0;
        for(long long i=0;i<n;i++){
            if(arr[i]>arr[i+1]){
                k=1;
                break;
            }
        }
        if(k==1){
            cout<<"NO"<<endl;
        }
        else{
            cout<<"YES"<<endl;
        }
    }
}
